QWidget { 
  background-color: #d5d5d5;
  color: #2b2b2b;
  font-size: 16px;
}

QPushButton { 
  background-color: #555;
  border: 3px solid #000000;
  border-radius: 5px;
  padding: 5px;
  color: #ffffff;
}

QPushButton:hover { 
  background-color: #666;
}

QFrame#leftMotorFrame,
QFrame#rightMotorFrame,
QFrame#pinFrame {
    border: 4px solid #666;
    border-radius: 4px;
}



QPushButton#estopButton {
  color: black;
}

QLabel#dateTimeLabel {
  font-size: 24px;
}